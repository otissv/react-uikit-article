{
  "name": "react-uikit-article",
  "version": "1.0.2",
  "description": "React uikit article component to create articles within a page.",
  "keywords": [
    "javascript",
    "react",
    "uikit",
    "react-component",
    "react-uikit",
    "uikit-react",
    "alert"
  ],
  "repository": "git@github.com:otissv/react-uikit-article.git",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "./lib/article.js",
  "scripts": {
    "build": "babel -d lib/ src/",
    "build:watch": "watch 'npm run build' ./src",
    "lint": "eslint --quiet ./src/**  || true",
    "lint:watch": "watch 'npm run lint' ./",
    "prepublish": "babel -d lib/ src/",
    "peer": "npm install cuid ufunc react react-dom react-uikit-base",
    "test": "babel-node test-runner.js | tap-min || true",
    "test:spec": "babel-node test-runner.js | tap-difflet || true",
    "test:watch": "watch 'npm run test' ./"
  },
  "devDependencies": {
    "babel-cli": "~6.11.4",
    "babel-core": "~6.13.2",
    "babel-eslint": "~6.1.2",
    "babel-plugin-transform-runtime": "~6.12.0",
    "babel-preset-es2015": "~6.13.2",
    "babel-preset-react": "~6.11.1",
    "babel-preset-stage-2": "~6.13.0",
    "babel-runtime": "~6.11.6",
    "cheerio": "~0.20.0",
    "eslint": "~3.2.2",
    "eslint-config-standard": "~5.3.5",
    "eslint-plugin-react": "~6.0.0",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~2.0.0",
    "eslint-plugin-promise": "~2.0.1",
    "faucet": "~0.0.1",
    "glob": "~7.0.5",
    "highlight.js": "~9.6.0",
    "history": "~3.0.0",
    "react-addons-test-utils": "~15.3.0",
    "tap-difflet": "~0.6.0",
    "tap-min": "~1.1.0",
    "tape": "~4.6.0",
    "watch": "~0.19.1"
  },
  "peerDependencies": {
    "cuid": "~1.3.8",
    "react": "~15.3.0",
    "react-dom": "~15.3.0",
    "react-uikit-base": "~1.0.5",
    "ufunc": "~1.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "engines": {
    "node": "~4.4.3"
  }
}
